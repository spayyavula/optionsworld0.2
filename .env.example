# Polygon.io API Configuration
VITE_POLYGON_API_KEY=your_polygon_api_key_here

# Application Configuration
VITE_APP_NAME=Paper Trading Platform
VITE_APP_VERSION=1.0.0

# Development Configuration
VITE_DEV_MODE=true
VITE_ENABLE_MOCK_DATA=true

# API Endpoints
VITE_POLYGON_BASE_URL=https://api.polygon.io

# Options Trading Configuration
VITE_OPTIONS_UPDATE_INTERVAL=5000
VITE_MAX_HISTORICAL_DAYS=14
VITE_DEFAULT_PORTFOLIO_VALUE=100000

# Feature Flags
VITE_ENABLE_OPTIONS_TRADING=true
VITE_ENABLE_REAL_TIME_DATA=false
VITE_ENABLE_ADVANCED_CHARTS=true

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Stripe Configuration (for future premium features)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here
VITE_STRIPE_MONTHLY_PRICE_ID=price_monthly_subscription_id
VITE_STRIPE_YEARLY_PRICE_ID=price_yearly_subscription_id
VITE_STRIPE_COFFEE_PRICE_ID=price_coffee_one_time_id

# Coupon System Configuration
VITE_ENABLE_COUPONS=true
VITE_MAX_COUPON_DISCOUNT=90
VITE_DEFAULT_COUPON_USAGE_LIMIT=1000
# Constant Contact Configuration
VITE_CONSTANT_CONTACT_API_KEY=your_constant_contact_api_key_here
VITE_CONSTANT_CONTACT_ACCESS_TOKEN=your_constant_contact_access_token_here
VITE_CONSTANT_CONTACT_LIST_ID=your_constant_contact_list_id_here

# Buy Me a Coffee Configuration
VITE_BUY_ME_COFFEE_USERNAME=your_buymeacoffee_username
VITE_BUY_ME_COFFEE_WIDGET_ID=your_widget_id_here

# Community Platform Configuration
VITE_SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/slack/webhook
VITE_DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your/discord/webhook
VITE_TELEGRAM_BOT_TOKEN=your_telegram_bot_token
VITE_TELEGRAM_CHAT_ID=your_telegram_chat_id
VITE_TELEGRAM_CHANNEL=your_telegram_channel_username
VITE_WHATSAPP_GROUP_INVITE=your_whatsapp_group_invite_link
VITE_FACEBOOK_GROUP_ID=your_facebook_group_id

# Community Integration Configuration
# For Slack: Create a Slack app and enable Incoming Webhooks
# For Discord: Server Settings > Integrations > Webhooks
# For Telegram: Create a bot with @BotFather and get the token
# For WhatsApp: Create a group and get the invite link
# For Facebook: Create a group and get the group ID

# Database Configuration
VITE_ENABLE_DATA_PERSISTENCE=true
VITE_HISTORICAL_DATA_RETENTION_DAYS=30


